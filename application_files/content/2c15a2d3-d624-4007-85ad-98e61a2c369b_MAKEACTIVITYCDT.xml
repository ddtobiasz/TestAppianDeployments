<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<contentHaul xmlns:a="http://www.appian.com/ae/types/2009">
    <versionUuid>_a-0000e0cb-9c1d-8000-29b4-01ef9001ef90_22310</versionUuid>
    <rule>
        <name>QACC_CreateActivityCDT</name>
        <uuid>2c15a2d3-d624-4007-85ad-98e61a2c369b_MAKEACTIVITYCDT</uuid>
        <description>Formatting rule that generates the activity history for a particular process [Activity History]</description>
        <parentUuid>c2b7d266-2844-4432-bc65-200cc7c30ca3</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>'type!{urn:com:appian:types:QACC}QACC_Activity'(
  recordId: ri!recordId,
  who: ri!who,
  action: ri!action,
  /* Use a different Details pattern for each type of activity */
  details:if(
    ri!action = #"_a-0000e0cb-9c1d-8000-29b4-01ef9001ef90_21937",
    "To: " &amp; #"1fd6d656-cae1-4f61-9097-13cf3ce91e21_FORMATNAME"(ri!taskAssignee) &amp; char(10) &amp; "Details: " &amp; ri!taskDetails &amp; if(
      isnull(ri!taskDeadline),
      "",
      char(10) &amp; "Deadline: " &amp; ri!taskDeadline
    ),
    if(
      or(ri!action = #"8f482d6f-47a8-4539-9fc2-a7347bf895b5_ACTIONCOMPLETED", ri!action = #"73ffb098-ab59-4022-966a-d16848a5cc3a_ACTIONSKIPPED"),
      "From: " &amp; #"1fd6d656-cae1-4f61-9097-13cf3ce91e21_FORMATNAME"(ri!taskAssigner) &amp; char(10) &amp; if(isnull(ri!comments), "(No Comments)", "Comments: " &amp; ri!comments),
      if(
        ri!action = #"_a-0000e0cb-9c1d-8000-29b4-01ef9001ef90_21933",
        "Message: " &amp; ri!message &amp; if(not(isnull(ri!attachment)), char(10) &amp; "Attachment: " &amp; #"_a-0000e0cb-9c1d-8000-29b4-01ef9001ef90_21993"(ri!attachment), ""),
        if(
          or(ri!action = #"_a-0000e0cb-9c1d-8000-29b4-01ef9001ef90_21929", ri!action = #"_a-0000e0cb-9c1d-8000-29b4-01ef9001ef90_21945"),
          #"_a-0000e0cb-9c1d-8000-29b4-01ef9001ef90_22033"(ri!oldCDT, ri!newCDT, #"_a-0000e0cb-9c1d-8000-29b4-01ef9001ef90_22117", #"_a-0000e0cb-9c1d-8000-29b4-01ef9001ef90_22113", #"_a-0000e0cb-9c1d-8000-29b4-01ef9001ef90_22121") &amp;
            if(
              or(isnull(ri!newDocs), count(ri!newDocs)=0),
              "",
              "Attached Files: " &amp; joinarray(apply(#"_a-0000e0cb-9c1d-8000-29b4-01ef9001ef90_21993", ri!newDocs), ", ") &amp; char(10)
            ) &amp;
            if(or(isnull(ri!deletedDocs), count(ri!deletedDocs)=0),
              "",
              "Deleted Files: " &amp; joinarray(apply(#"_a-0000e0cb-9c1d-8000-29b4-01ef9001ef90_21993", ri!deletedDocs), ", ")
            ),
          "no activity found"
        )
      )
    )
  ),
  when: now()
)</definition>
        <namedTypedValue>
            <name>recordId</name>
            <type>
                <name>int</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>who</name>
            <type>
                <name>User</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>action</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <namedTypedValue>
            <name>taskAssignee</name>
            <type>
                <name>User</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>taskAssigner</name>
            <type>
                <name>User</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>taskDetails</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <namedTypedValue>
            <name>taskDeadline</name>
            <type>
                <name>dateTime</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>comments</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <namedTypedValue>
            <name>message</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <namedTypedValue>
            <name>attachment</name>
            <type>
                <name>CollaborationDocument</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>oldCDT</name>
            <type>
                <name>QACC_QuickAppCalledCrap</name>
                <namespace>urn:com:appian:types:QACC</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>newCDT</name>
            <type>
                <name>QACC_QuickAppCalledCrap</name>
                <namespace>urn:com:appian:types:QACC</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>newDocs</name>
            <type>
                <name>CollaborationDocument?list</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>deletedDocs</name>
            <type>
                <name>CollaborationDocument?list</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
            </type>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
